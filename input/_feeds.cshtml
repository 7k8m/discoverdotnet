@{
	Layout = null;
}
<opml version="1.0">
  <head>
    <title>Discover .NET Feeds</title>
  </head>
  <body>
    <outline text="Discover .NET Feeds" title="Discover .NET Feeds">
      @foreach(IDocument blog in Outputs["Blogs"])
      {
        <outline type="rss" text="@blog["Title"]" title="@blog["Title"]" xmlUrl="@blog["Feed"]" />
      }
      @foreach(IDocument broadcast in Outputs["Broadcasts"])
      {
        <outline type="rss" text="@broadcast["Title"]" title="@broadcast["Title"]" xmlUrl="@broadcast["Feed"]" />
      }
    </outline>
  </body>
</opml>